<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="local.ManualOpenProxy">
    <method name="Open">
      <arg name="app_name" type="s" direction="in"/>
    </method>
    <method name="OpenTitle">
      <arg name="app_name" type="s" direction="in"/>
      <arg name="title_name" type="s" direction="in"/>
    </method>
    <method name="ShowManual">
      <arg name="app_name" type="s" direction="in"/>
    </method>
    <method name="Search">
      <arg name="keyword" type="s" direction="in"/>
    </method>
    <method name="FilesUpdate">
      <arg name="filesList" type="as" direction="in"/>
    </method>
  </interface>
</node>
